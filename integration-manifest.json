{
  "$schema": "https://keyfactor.github.io/integration-manifest-schema.json",
  "integration_type": "orchestrator",
  "name": "Kubernetes Orchestrator Extension",
  "status": "production",
  "description": "The Kubernetes Orchestrator allows for the remote management of certificate stores defined in a Kubernetes cluster. The following types of Kubernetes resources are supported:\n- Secrets - Kubernetes secrets of type `kubernetes.io/tls` or `Opaque` \n- Certificates - Kubernetes certificates of type `certificates.k8s.io/v1`",
  "short_description": "The Kubernetes Orchestrator allows for remote management of Kubernetes secret types 'Opaque' and 'kubernetes.io/tls' as well as 'certificates.k8s.io/v1' resources. ",
  "link_github": false,
  "topics": [
    "kubernetes",
    "certificates",
    "secrets",
    "k8s",
    "aks",
    "eks",
    "gke",
    "keyfactor",
    "orchestrator",
    "keyfactor-universal-orchestrator"
  ],
